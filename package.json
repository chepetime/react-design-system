{
  "private": false,
  "name": "react-turborepo-dls",
  "version": "1.0.3",
  "description": "Design System with React, Turborepo, Storybook and NextJs",
  "license": "MIT",
  "author": "Jos√© Manuel Gulias Lugo <me@joselugo.dev>",
  "homepage": "https://github.com/chepetime/react-turborepo-dls-ops#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chepetime/react-turborepo-dls-ops.git"
  },
  "bugs": {
    "url": "https://github.com/chepetime/react-turborepo-dls-ops/issues"
  },
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "turbo run build --filter=@chepe/ui --filter=@chepe/icons",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "npm run storybook",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate:component": "turbo run generate:component --filter=generator",
    "lint": "turbo run lint",
    "storybook": "turbo run storybook --filter=docs",
    "storybook-build": "turbo run storybook-build --filter=docs",
    "storybook-test": "turbo run storybook-test --filter=docs",
    "test": "turbo run test --parallel --filter=@chepe/ui",
    "web": "turbo run dev --parallel --filter=web --filter=@chepe/ui--filter=@chepe/icons"
  },
  "workspaces": [
    "apps/*",
    "config/*",
    "tools/*",
    "packages/*"
  ],
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.8.4",
    "turbo": "^1.8.3"
  },
  "keywords": [],
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=9.3.1"
  },
  "packageManager": "npm@9.3.1"
}
