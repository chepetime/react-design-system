{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "storybook:dev": {
      "outputs": ["dist/**"],
      "dependsOn": ["^dev"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "token": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    }
  }
}
