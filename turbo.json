{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "storybook": {
      "cache": false
    },
    "storybook-build": {
      "cache": false,
      "outputs": ["build/**"]
    },
    "storybook-test": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "web": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "generate:component": {
      "cache": false
    },
    "token-engine:fetch": {
      "cache": false,
      "dependsOn": ["build"],
      "outputs": ["tokens/**"]
    }
  }
}
