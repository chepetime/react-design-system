  "scripts": {
    "build": "npm run storybook-build",
    "build-storybook": "storybook build",
    "clean": "rm -rf .turbo && rm -rf storybook-static && rm -rf node_modules",
    "dev": "npm run storybook",
    "preview-storybook": "serve storybook-static",
    "serve": "storybook build && serve storybook-static",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build --docs",
    "storybook-test": "test-storybook --no-cache --verbose --url http://127.0.0.1:6006",
    "test": "npx concurrently -k -s first -n \"Server,Test\" -c \"magenta,blue\" \"http-server ./storybook-static --port 6006 --silent\" \"wait-on http-get://127.0.0.1:6006 && npm run storybook-test\""
  },
