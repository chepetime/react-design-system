{
  "private": true,
  "name": "dex-dls",
  "version": "0.0.0",
  "description": "DEX DLS is the Design System for General Electric Vernova",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "storybook:dev": "turbo run storybook:dev --parallel",
    "token": "turbo run token"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "npm@8.11.0"
}
